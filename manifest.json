{
    "manifest_version": 3,
    "name": "yqn",
    "version": "2.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "logo.png"
    },
    "host_permissions": [
      "*://*.google.com.hk/",
      "*://*.yqn.com/"
    ],
    "permissions": ["tabs","storage","cookies"],
    "background": {
      "service_worker": "background.js"
    }
  }
  